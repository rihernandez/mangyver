{
  "name": "fixmaint",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "env": "echo $ENV_TOKEN | sed -e 's/^.//' | base64 -d >>.env",
    "preinstall": "npm run env:check",
    "postinstall": "npx husky-init && touch .husky/pre-commit && npm run precommit:replace && npm run env",
    "precommit:replace": "sed -i.bank 's/npm test/npx lint-staged/g' '.husky/pre-commit'",
    "env:check": "if [ ! $ENV_TOKEN ]; then clear; echo 'variable $ENV_TOKEN is not set'; exit 2; fi",
    "start": "npm run env:check && node build/src/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "prod": "npm run build && node build/src/index.js",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "jest",
    "prettier": "prettier . --write --ignore-unknown",
    "prettier:check": "prettier . --check",
    "lint": "eslint . --ext .ts; exit 0",
    "lint-debug": "eslint --debug; exit 0",
    "lint-fix": "eslint . --fix; exit 0",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Richard HC",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.8",
    "@types/express-pino-logger": "^4.0.3",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^5.3.0",
    "eslint": "^8.2.0",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.0.3",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "husky": "^7.0.0"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/helmet": "^4.0.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "mssql": "^7.1.3",
    "password-validator": "^5.1.1",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "tslog": "^3.2.2",
    "tsoa": "^3.3.0",
    "typeorm": "^0.2.29",
    "typescript-logger": "^5.0.1"
  },
  "engines": {
    "node": "12.22.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
